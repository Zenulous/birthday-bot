{
  "name": "birthday-bot-discord",
  "version": "1.0.0",
  "description": "A simple birthday bot for Discord",
  "scripts": {
    "start": "node ./lib/app.js",
    "postinstall": "tsc",
    "check": "tsc && gts check",
    "fix": "gts fix",
    "develop": "tsc-watch --onSuccess \"node ./lib/app.js\""
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run check"
    }
  },
  "author": "Zen van Riel",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lowdb": "^1.0.0",
    "moment": "^2.24.0",
    "node-schedule": "^1.3.2",
    "gts": "^1.1.2",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/node": "^13.5.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.149",
    "@types/lowdb": "^1.0.9",
    "@types/node-schedule": "^1.3.0",
    "@discordjs/uws": "^10.149.0",
    "@types/request-promise": "^4.1.45",
    "husky": "^4.2.1",
    "prettier": "^1.19.1",
    "tslint": "^6.0.0",
    "tsc-watch": "^4.1.0"
  }
}
